<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:tag="layout/rounded_multiple_images_view_0" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView android:id="@+id/one_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop"/>
    <RelativeLayout android:id="@+id/two_images" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/two_images_left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/two_images_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentEnd="true"/>
        <View android:background="@color/whiteThree" android:layout_width="1dp" android:layout_height="match_parent" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/three_images" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/three_images_left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/three_images_upper_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentEnd="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/three_images_bottom_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentBottom="true" android:layout_alignParentEnd="true"/>
        <View android:background="@color/whiteThree" android:layout_width="1dp" android:layout_height="match_parent" android:layout_centerHorizontal="true"/>
        <View android:id="@+id/three_images_horizontal_border" android:background="@color/whiteThree" android:layout_width="match_parent" android:layout_height="1dp" android:layout_centerVertical="true" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/four_images" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/four_images_upper_left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/four_images_upper_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_alignParentEnd="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/four_images_bottom_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentBottom="true" android:layout_alignParentEnd="true"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/four_images_bottom_left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="centerCrop" android:layout_alignParentBottom="true" android:layout_alignParentStart="true"/>
        <View android:background="@color/whiteThree" android:layout_width="1dp" android:layout_height="match_parent" android:layout_centerHorizontal="true"/>
        <View android:background="@color/whiteThree" android:layout_width="match_parent" android:layout_height="1dp" android:layout_centerVertical="true"/>
    </RelativeLayout>
</FrameLayout>
