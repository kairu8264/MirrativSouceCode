<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="0dp" android:thickness="3dp">
            <solid android:color="@color/tealish"/>
            <size android:height="6dp" android:width="13dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="0dp" android:thickness="3dp">
            <solid android:width="1dp" android:color="@color/whiteThree"/>
            <size android:height="6dp" android:width="13dp"/>
        </shape>
    </item>
</selector>
